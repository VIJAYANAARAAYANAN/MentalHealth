{
  "expo": {
    "name": "MindBox",
    "slug": "MindBox",
    "version": "1.0.0",
    "orientation": "portrait",
    "plugins": [
      [
        "expo-font",
        {
          "Montserrat-Black": "./src/fonts/Montserrat-Black.ttf"
        }
      ]
    ],
    "splash": {
      "backgroundColor": "#002D62",
      "text": "MindBox"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/newleaf.png",
        "backgroundColor": "#002D62"
      },
      "package": "com.vijay.MindBox"
    },
    "scheme": [
      "mindbox"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "be43d538-6341-417a-b3e9-cb59de4e1c4a"
      }
    }
  }
}
